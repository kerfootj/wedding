{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": ["source.sortImports", "source.fixAll.eslint"],
    "eslint.options": {
        "fixTypes": ["problem", "suggestion", "layout"]
    },
    "cSpell.words": [
        "haviland",
        "kamloops",
        "leia",
        "nocache",
        "noimageindex",
        "tailwindcss",
        "varela",
        "webp"
    ],
    "fontAwesomeAutocomplete.patterns": ["**/*.tsx", "**/*.html"],
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
