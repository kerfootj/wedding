{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": ["source.sortImports", "source.fixAll.eslint"],
    "eslint.options": {
        "fixTypes": ["problem", "suggestion", "layout"]
    },
    "cSpell.words": [
        "Aliya",
        "Bieber",
        "Derker",
        "Forshner",
        "haviland",
        "Heale",
        "Hinterleitner",
        "Huska",
        "kamloops",
        "Kerfoot",
        "Koby",
        "Kosten",
        "leia",
        "nocache",
        "noimageindex",
        "Schober",
        "sendgrid",
        "Shael",
        "Steph",
        "tailwindcss",
        "Tannant",
        "Trish",
        "varela",
        "webp",
        "westsyde"
    ],
    "fontAwesomeAutocomplete.patterns": ["**/*.tsx", "**/*.html"]
}
